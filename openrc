    /bin/rmdir						mr,
  }
  
  profile mount /bin/mount {
    #include <abstractions/base>
    #include <local/profile_blocks/openrc.root.d/>		# Локальные правила монтирования и размонтирования.
    
    # CAPABILITIES -------------------------------------
    capability sys_admin,
    
    # PSEUDO -------------------------------------------
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /sys/devices/virtual/block/@{BLOCK_VIRT}/{,**}	r,
    @{PROC}/@{pid}/mount{s,info}			r,
    
    # EXECUTABLES --------------------------------------
    /bin/mount						mr,
    
    # READS/WRITES -------------------------------------
    /							r,
    /etc/fstab						r,
  }
}

