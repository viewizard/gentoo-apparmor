profile openrc.mark_service_inactive /lib{,32,64}/rc/sbin/mark_service_inactive flags=(complain) {
  #include <abstractions/base>
  
  # SIGNAL ---------------------------------------------
  signal (send) set=(hup) peer="*.init.d",
  
  # PSEUDO ---------------------------------------------
  /dev/tty[0-9]*					rw,
  /dev/console						rw,
  /dev/pts/[0-9]*					rw,
  
  # EXECUTABLES ----------------------------------------
  /lib{,32,64}/rc/sbin/mark_service_inactive		mr,
  
  # READS/WRITES ---------------------------------------
  /run/openrc/inactive/*				w,
  /run/openrc/starting/*				w,
  /run/openrc/exclusive/*				w,
  /run/openrc/stopping/*				w,
}

profile openrc.netmount.init.d /etc/init.d/netmount {
  #include <abstractions/base>
  #include <abstractions/nameservice>
  #include <abstractions/openrc>
  
  # CAPABILITIES ---------------------------------------
  capability mknod,
  
  # PSEUDO ---------------------------------------------
  /sys/fs/cgroup/openrc/netmount/tasks			w,
  /sys/fs/cgroup/unified/netmount/cgroup.procs		w,
  /sys/fs/cgroup/unified/netmount/cgroup.events		r,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						Cx,
  /etc/init.d/dnsmasq					Px,
  /bin/mkdir						Cx,
  /bin/grep						Cx,
  /bin/rmdir						Cx,
  /bin/mount						Cx,
  /bin/umount						Cx,
  
  # READS/WRITES ---------------------------------------
  /etc/{conf,init}.d/netmount				r,
  
  profile shell @{shell} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions/openrc-shell>
  }
  
  profile mkdir /bin/mkdir {
    #include <abstractions/base>
    
    # CAPABILITIES -------------------------------------
    capability dac_override,
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /sys/fs/cgroup/{openrc,unified}/netmount/		w,
    
    # EXECUTABLES --------------------------------------
    /bin/mkdir						mr,
  }
  
  profile grep /bin/grep {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /sys/fs/cgroup/unified/netmount/cgroup.procs	r,
    
    # EXECUTABLES --------------------------------------
    /bin/grep						mr,
  }
  
  profile rmdir /bin/rmdir {
    #include <abstractions/base>
    
    # CAPABILITIES -------------------------------------
    capability dac_override,
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /sys/fs/cgroup/unified/netmount/			w,
    
    # EXECUTABLES --------------------------------------
    /bin/rmdir						mr,
  }
  
  profile mount /bin/mount {
    #include <abstractions/base>
    
    # MOUNT --------------------------------------------
    
    # EXECUTABLES --------------------------------------
    /bin/mount						mr,
    
    # READS/WRITES -------------------------------------
    /etc/fstab						r,
  }
  
  profile umount /bin/umount {
    #include <abstractions/base>
    
    # MOUNT --------------------------------------------
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    @{PROC}/@{pid}/mountinfo				r,
    
    # EXECUTABLES --------------------------------------
    /bin/umount						mr,
  }
}

