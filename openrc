  /sys/fs/cgroup/unified/modules-load/cgroup.events	r,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						Cx,
  /bin/kmod						Px,
  /bin/sort						Cx,
  /bin/mkdir						Cx,
  /bin/grep						Cx,
  /bin/rmdir						Cx,
  
