    /sys/fs/cgroup/unified/keymaps/			w,
    
    # EXECUTABLES --------------------------------------
    /bin/rmdir						mr,
  }
}

profile openrc.urandom.init.d /etc/init.d/urandom {
  #include <abstractions/base>
  #include <abstractions/nameservice>
  #include <abstractions/openrc>
  
  # CAPABILITIES ---------------------------------------
  capability mknod,
  
  # PSEUDO ---------------------------------------------
  /dev/urandom						w,
  /sys/fs/cgroup/openrc/urandom/tasks			w,
  /sys/fs/cgroup/unified/urandom/cgroup.procs		w,
  /sys/fs/cgroup/unified/urandom/cgroup.events		r,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						Cx,
  /bin/cat						Cx,
  /bin/dd						Cx,
  /bin/rm						Cx,
  /bin/mkdir						Cx,
  /bin/grep						Cx,
  /bin/rmdir						Cx,
  
  # READS/WRITES ---------------------------------------
  /etc/{conf,init}.d/urandom				r,
  
  profile shell @{shell} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions/openrc-shell>
  }
  
  profile cat /bin/cat {
    #include <abstractions/base>
    
    # CAPABILITIES -------------------------------------
    capability mknod,
    
    # PSEUDO -------------------------------------------
    /dev/urandom					w,
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    @{PROC}/sys/kernel/random/poolsize			r,
    
    # EXECUTABLES --------------------------------------
    /bin/cat						mr,
    
    # READS/WRITES -------------------------------------
    /var/lib/misc/random-seed				r,
  }
  
  profile dd /bin/dd {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    
    # EXECUTABLES --------------------------------------
