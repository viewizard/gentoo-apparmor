profile openrc.devfs.init.d /etc/init.d/devfs {
  #include <abstractions/base>
  #include <abstractions/nameservice>
  #include <abstractions/openrc>
  
  # CAPABILITIES ---------------------------------------
  capability mknod,
  
  # PSEUDO ---------------------------------------------
  /sys/fs/cgroup/openrc/devfs/tasks			w,
  /sys/fs/cgroup/unified/devfs/cgroup.procs		w,
  /sys/fs/cgroup/unified/devfs/cgroup.events		r,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						Cx,
  /bin/mkdir						Cx,
  /bin/grep						Cx,
  /bin/rmdir						Cx,
  /bin/mount						Cx,
  
  # READS/WRITES ---------------------------------------
  /etc/{conf,init}.d/devfs				r,
  
  profile shell @{shell} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions/openrc-shell>
  }
  
  profile mkdir /bin/mkdir {
    #include <abstractions/base>
    
    # CAPABILITIES -------------------------------------
    capability dac_override,
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /dev/mqueue/					rw,
    /sys/fs/cgroup/{openrc,unified}/devfs/		w,
    
    # EXECUTABLES --------------------------------------
    /bin/mkdir						mr,
  }
  
  profile grep /bin/grep {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /sys/fs/cgroup/unified/devfs/cgroup.procs		r,
    
    # EXECUTABLES --------------------------------------
    /bin/grep						mr,
  }
  
  profile rmdir /bin/rmdir {
    #include <abstractions/base>
    
    # CAPABILITIES -------------------------------------
    capability dac_override,
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    /sys/fs/cgroup/unified/devfs/			w,
    
    # EXECUTABLES --------------------------------------
    /bin/rmdir						mr,
  }
  
  profile mount /bin/mount {
    #include <abstractions/base>
    
    # CAPABILITIES -------------------------------------
    capability sys_admin,
    
    # MOUNT --------------------------------------------
    mount options=(rw, nosuid, remount) -> /dev/,
    mount fstype=mqueue options=(rw, nosuid, nodev, noexec) mqueue -> /dev/mqueue/,
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/console					rw,
    /dev/pts/[0-9]*					rw,
    
    # EXECUTABLES --------------------------------------
    /bin/mount						mr,
  }
}

