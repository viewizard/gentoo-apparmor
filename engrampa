# DELEGATION! На данный момент не можем вынести в отдельные профили все архиваторы,
# т.к. кол-во переключений на именованные профили не может превышать 12, с root
# будут проблемы (нужно делать больше 12 переключений профилей).

#include <local/tunables.d/>

# предустановка для abstractions/fs/access-by-user-preset-rwkl
@{USERS_DIR_rwkl}="[^.]**"
@{USERS_FILE_rwkl}="[^.]**"

profile engrampa@{p_user} /usr/bin/engrampa@{t_user} {
  #include <abstractions/base>
  #include <abstractions-user/freedesktop.org>
  #include <abstractions-user/fonts>
  #include <abstractions-user/gtk>
  #include <abstractions-user/video>
  #include <abstractions-user/X>
  #include <abstractions/nameservice>
  #include <abstractions-user/xdg-open>
  #include <abstractions-user/dconf>
  #include <abstractions/dbus/system-strict>
  #include <abstractions/dbus/session-strict>
  #include <abstractions/dbus/accessibility-strict>
  #include <abstractions/dbus/at-spi>
  #include <abstractions/dbus/gvfs>
  #include <abstractions-user/fs/access-by-users-preset-rwkl>
  #include <abstractions/fs/deny-private-strict>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/unix/gvfsd-trash>
  #include <abstractions/unix/gvfsd-network>
  #include <abstractions-user/profile_transitions/allow-mime-related>
  
  # SIGNAL ---------------------------------------------
  signal (send) set=(term) peer="engrampa@{p_user}//*",
  
  # PSEUDO ---------------------------------------------
  /dev/tty						rw,
  /dev/tty[0-9]*					rw,
  /dev/pts/[0-9]*					rw,
  
  # EXECUTABLES ----------------------------------------
  /usr/bin/engrampa					mr,
  @{shell}@{t_user}					Cx,
  /bin/mv@{t_user}					Cx,	# sys-apps/coreutils
  /bin/rm@{t_user}					Cx,	# sys-apps/coreutils
  /bin/cp@{t_user}					Cx,	# sys-apps/coreutils
  /bin/tar@{t_user}					Cx,	# app-arch/tar
  /bin/bzip2@{t_user}					Cx,	# app-arch/bzip2
  /bin/gzip@{t_user}					Cx,	# app-arch/gzip
  /usr/bin/7z@{t_user}					Cx,	# app-arch/p7zip
  /usr/bin/7za@{t_user}					Cx,	# app-arch/p7zip
  /usr/bin/zip@{t_user}					Cx,	# app-arch/zip
  /usr/bin/unzip@{t_user}				Cx,	# app-arch/unzip
  /usr/bin/lzip@{t_user}				Cx,	# app-arch/lzip
  /usr/bin/xz@{t_user}					Cx,	# app-arch/xz-utils
  /usr/bin/dpkg-deb@{t_user}				Px -> engrampa_dpkg_deb,	# app-arch/dpkg
  /usr/@{CHOST}/binutils-bin/[0-9]*/ar@{t_user}		Cx,	# sys-devel/binutils
  /opt/rar/rar@{t_user}					Cx,	# app-arch/rar
  
  # READS/WRITES ---------------------------------------
  /usr/share/engrampa/{,**}				r,
  
  # USERS ----------------------------------------------
  owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
  owner @{USER_HOME_D}/.config/mimeapps.list*		rw,
  owner @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  
  profile shell @{shell}@{t_user} {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty						rw,
    
    # EXECUTABLES --------------------------------------
    @{shell}						mr,
    /usr/libexec/engrampa/rpm2cpio@{t_user}		Px -> engrampa_rpm2cpio,
  }
  
  profile mv /bin/mv@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/mv						mr,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile rm /bin/rm@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/pts/[0-9]*					rw,
    
    # EXECUTABLES --------------------------------------
    /bin/rm						mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile cp /bin/cp@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/cp						mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile tar /bin/tar@{t_user} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/tar						mr,
    @{shell}@{t_user}					Px -> engrampa_tar_shell,
    /bin/bzip2@{t_user}					ix,	# app-arch/bzip2	# Oставляем запуск через ix.
    /bin/gzip@{t_user}					ix,	# app-arch/gzip		# Oставляем запуск через ix.
    /usr/bin/lzip@{t_user}				ix,	# app-arch/lzip		# Oставляем запуск через ix.
    /usr/bin/xz@{t_user}				ix,	# app-arch/xz-utils	# Oставляем запуск через ix.
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile bzip2 /bin/bzip2@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/bzip2						mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile gzip /bin/gzip@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/gzip						mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile 7z /usr/bin/7z@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # PSEUDO -------------------------------------------
    /dev/tty						rw,
    @{SYS_D}/devices/system/cpu/			r,
    
    # EXECUTABLES --------------------------------------
    /usr/bin/7z						r,
    @{shell}						mr,
    /usr/lib{,32,64}/p7zip/7z@{t_user}			ix,	# app-arch/p7zip	# Все правильно, для 7z используем с ix.
    
    # READS/WRITES -------------------------------------
    /usr/lib64/p7zip/7z.so				m,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile 7za /usr/bin/7za@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # PSEUDO -------------------------------------------
    /dev/tty						rw,
    @{SYS_D}/devices/system/cpu/			r,
    
    # EXECUTABLES --------------------------------------
    /usr/bin/7za					r,
    @{shell}						mr,
    /usr/lib{,32,64}/p7zip/7za@{t_user}			ix,	# app-arch/p7zip	# Все правильно, для 7za используем с ix.
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile zip /usr/bin/zip@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/zip					mr,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile unzip /usr/bin/unzip@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/unzip					mr,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile lzip /usr/bin/lzip@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/lzip					mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile xz /usr/bin/xz@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/xz						mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile ar /usr/@{CHOST}/binutils-bin/[0-9]*/ar@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/@{CHOST}/binutils-bin/[0-9]*/ar		mr,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
  
  profile rar /opt/rar/rar@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /opt/rar/rar					mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # NOISY --------------------------------------------
    deny /etc/						r,
    deny /usr/{,local/}lib{,32,64}/			r,
    
    # DENY INHERIT -------------------------------------
    deny @{USER_HOME_D}/.local/share/gvfs-metadata/{,*}	rw,	# gnome-base/gvfs
  }
}

# IMPROVE! Сейчас (модуль ядра 3.6, утилита 2.11.0) не поддерживаются
# множественные вложенные профили, выносим в отдельный профиль.
profile engrampa_rpm2cpio {
  #include <abstractions/base>
  #include <abstractions-user/fs/access-by-users-preset-rwkl>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  
  # EXECUTABLES ----------------------------------------
  /usr/libexec/engrampa/rpm2cpio			mr,
  @{shell}@{t_user}					Cx,
  
  profile shell @{shell}@{t_user} {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty						rw,
    
    # EXECUTABLES --------------------------------------
    @{shell}						mr,
    @{shell}@{t_user}					Px -> engrampa_rpm2cpio_shell,
  }
}

# IMPROVE! Сейчас (модуль ядра 3.6, утилита 2.11.0) не поддерживаются
# множественные вложенные профили, выносим в отдельный профиль.
profile engrampa_rpm2cpio_shell {
  #include <abstractions/base>
  #include <abstractions-user/fs/access-by-users-preset-rwkl>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  
  # PSEUDO ---------------------------------------------
  /dev/tty						rw,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						mr,
  /bin/dd@{t_user}					Cx,	# sys-apps/coreutils
  /bin/gzip@{t_user}					Cx,	# app-arch/gzip
  /bin/cpio@{t_user}					Cx,	# app-arch/cpio
  
  profile dd /bin/dd@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/dd						mr,
  }
  
  profile gzip /bin/gzip@{t_user} {
    #include <abstractions/base>
    
    # EXECUTABLES --------------------------------------
    /bin/gzip						mr,
  }
  
  profile cpio /bin/cpio@{t_user} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/cpio						mr,
  }
}

# IMPROVE! Сейчас (модуль ядра 3.6, утилита 2.11.0) не поддерживаются
# множественные вложенные профили, выносим в отдельный профиль.
profile engrampa_dpkg_deb {
  #include <abstractions/base>
  #include <abstractions-user/fs/access-by-users-preset-rwkl>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  
  # EXECUTABLES ----------------------------------------
  /usr/bin/dpkg-deb					mr,
  /bin/rm@{t_user}					Cx,	# sys-apps/coreutils
  /bin/tar@{t_user}					Cx,	# app-arch/tar
  
  # USERS ----------------------------------------------
  owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
  
  # TEMP -----------------------------------------------
  owner /tmp/dpkg-deb.@{TMP6}/				rw,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  owner /tmp/dpkg-deb.@{TMP6}/*				r,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  
  profile rm /bin/rm@{t_user} {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/rm						mr,
    
    # TEMP ---------------------------------------------
    owner /tmp/dpkg-deb.@{TMP6}/{,**}			rw,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  }
  
  profile tar /bin/tar@{t_user} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/tar						mr,
    
    # USERS --------------------------------------------
    owner @{USER_HOME_D}/.fr-@{TMP6}/{,**}		rw,
    
    # TEMP ---------------------------------------------
    owner /tmp/dpkg-deb.@{TMP6}/{,**}			w,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  }
}

# IMPROVE! Сейчас (модуль ядра 3.6, утилита 2.11.0) не поддерживаются
# множественные вложенные профили, выносим в отдельный профиль.
profile engrampa_tar_shell flags=(attach_disconnected) {
  #include <abstractions/base>
  #include <abstractions-user/fs/access-by-users-preset-rwkl>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  
  # PSEUDO ---------------------------------------------
  /dev/tty						rw,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						mr,
  /bin/gzip@{t_user}					Cx,	# app-arch/gzip
  /bin/bzip2@{t_user}					Cx,	# app-arch/bzip2
  /usr/bin/lzip@{t_user}				Cx,	# app-arch/lzip
  /usr/bin/xz@{t_user}					Cx,	# app-arch/xz-utils
  
  # DENY INHERIT ---------------------------------------
  deny /apparmor/.null					rw,	# flags=(attach_disconnected)
  
  profile gzip /bin/gzip@{t_user} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/gzip						mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
  
  profile bzip2 /bin/bzip2@{t_user} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/bzip2						mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
  
  profile lzip /usr/bin/lzip@{t_user} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/lzip					mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
  
  profile xz /usr/bin/xz@{t_user} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-user/fs/access-by-users-preset-rwkl>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/xz						mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
}

profile engrampa@{p_root} /usr/bin/engrampa@{t_root} {
  #include <abstractions/base>
  #include <abstractions-root/freedesktop.org>
  #include <abstractions-root/fonts>
  #include <abstractions-root/gtk>
  #include <abstractions-root/X>
  #include <abstractions-sudo/X-connection-strict>		# RBAC! Нужно для запуска под root-ом через sudo
  								# с подключением к X-сессии пользователя.
  #include <abstractions/nameservice>
  #include <abstractions/dbus/system-strict>
  #include <abstractions/dbus/session-strict>
  #include <abstractions/dbus/accessibility-strict>
  #include <abstractions/dbus/at-spi>
  #include <abstractions/dbus/gvfs>
  #include <abstractions-root/dconf>
  #include <abstractions-root/xdg-open>
  #include <abstractions-root/fs/access-by-users-preset-rwkl>
  #include <abstractions-root/fs/access-by-pattern-systemusers>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  #include <abstractions/unix/gvfsd-trash>
  #include <abstractions/unix/gvfsd-network>
  #include <abstractions-root/profile_transitions/allow-list>
  
  # SIGNAL ---------------------------------------------
  signal (send) set=(term) peer="engrampa@{p_root}//*",
  
  # PSEUDO ---------------------------------------------
  /dev/tty						rw,
  /dev/tty[0-9]*					rw,
  /dev/pts/[0-9]*					rw,
  
  # EXECUTABLES ----------------------------------------
  /usr/bin/engrampa					mr,
  @{shell}@{t_root}					Cx,
  /bin/mv@{t_root}					Cx,	# sys-apps/coreutils
  /bin/rm@{t_root}					Cx,	# sys-apps/coreutils
  /bin/cp@{t_root}					Cx,	# sys-apps/coreutils
  /bin/tar@{t_root}					Cx,	# app-arch/tar
  /bin/bzip2@{t_root}					Cx,	# app-arch/bzip2
  /bin/gzip@{t_root}					Cx,	# app-arch/gzip
  /usr/bin/7z@{t_root}					Cx,	# app-arch/p7zip
  /usr/bin/7za@{t_root}					Cx,	# app-arch/p7zip
  /usr/bin/zip@{t_root}					Cx,	# app-arch/zip
  /usr/bin/unzip@{t_root}				Cx,	# app-arch/unzip
  /usr/bin/lzip@{t_root}				Cx,	# app-arch/lzip
  /usr/bin/xz@{t_root}					Cx,	# app-arch/xz-utils
  /usr/bin/dpkg-deb@{t_root}				Px -> engrampa_dpkg_deb_root,	# app-arch/dpkg
  /usr/@{CHOST}/binutils-bin/[0-9]*/ar@{t_root}		Cx,	# sys-devel/binutils
  /opt/rar/rar@{t_root}					Cx,	# app-arch/rar
  
  # READS/WRITES ---------------------------------------
  /usr/share/engrampa/{,**}				r,
  
  # USERS ----------------------------------------------
  /root/.config/mimeapps.list*				rw,
  /root/.local/share/gvfs-metadata/{,*}			rw,	# gnome-base/gvfs
  /root/.cache/.fr-@{TMP6}/{,**}			rw,
  
  profile shell @{shell}@{t_root} {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty						rw,
    
    # EXECUTABLES --------------------------------------
    @{shell}						mr,
    /usr/libexec/engrampa/rpm2cpio@{t_root}		Px -> engrampa_rpm2cpio_root,
  }
  
  profile mv /bin/mv@{t_root} {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
