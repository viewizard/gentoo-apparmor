  }
  
  profile cp /bin/cp@{t_root} {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # CAPABILITIES -------------------------------------
    capability sys_admin,
    
    # EXECUTABLES --------------------------------------
    /bin/cp						mr,
    
    # USERS --------------------------------------------
    /root/.cache/.fr-@{TMP6}/{,**}			rw,
    
    # DENY INHERIT -------------------------------------
    deny /root/.local/share/gvfs-metadata/{,*}		rw,	# gnome-base/gvfs
  }
  
  profile tar /bin/tar@{t_root} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # CAPABILITIES ---------------------------------------
    capability chown,
    capability fowner,
    capability fsetid,
    
    # EXECUTABLES --------------------------------------
    /bin/tar						mr,
    @{shell}@{t_root}					Px -> engrampa_tar_shell_root,
    /bin/bzip2@{t_root}					ix,	# app-arch/bzip2	# Oставляем запуск через ix.
    /bin/gzip@{t_root}					ix,	# app-arch/gzip		# Oставляем запуск через ix.
    /usr/bin/lzip@{t_root}				ix,	# app-arch/lzip		# Oставляем запуск через ix.
    /usr/bin/xz@{t_root}				ix,	# app-arch/xz-utils	# Oставляем запуск через ix.
    
    # USERS --------------------------------------------
    /root/.cache/.fr-@{TMP6}/{,**}			rw,
    
    # DENY INHERIT -------------------------------------
    deny /root/.local/share/gvfs-metadata/{,*}		rw,	# gnome-base/gvfs
  }
  
  profile bzip2 /bin/bzip2@{t_root} {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
