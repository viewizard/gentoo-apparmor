# IMPROVE! Сейчас (модуль ядра 3.6, утилита 2.11.0) не поддерживаются
# множественные вложенные профили, выносим в отдельный профиль.
profile engrampa_dpkg_deb_root {
  #include <abstractions/base>
  #include <abstractions-root/fs/access-by-users-preset-rwkl>
  #include <abstractions-root/fs/access-by-pattern-systemusers>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  
  # EXECUTABLES ----------------------------------------
  /usr/bin/dpkg-deb					mr,
  /bin/rm@{t_root}					Cx,	# sys-apps/coreutils
  /bin/tar@{t_root}					Cx,	# app-arch/tar
  
  # USERS ----------------------------------------------
  /root/.cache/.fr-@{TMP6}/{,**}			rw,
  
  # TEMP -----------------------------------------------
  owner /tmp/dpkg-deb.@{TMP6}/				rw,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  owner /tmp/dpkg-deb.@{TMP6}/*				r,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  
  profile rm /bin/rm@{t_root} {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/rm						mr,
    
    # TEMP ---------------------------------------------
    owner /tmp/dpkg-deb.@{TMP6}/{,**}			rw,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  }
  
  profile tar /bin/tar@{t_root} {
    #include <abstractions/base>
    #include <abstractions/nameservice>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/tar						mr,
    
    # USERS --------------------------------------------
    /root/.cache/.fr-@{TMP6}/{,**}			rw,
    
    # TEMP ---------------------------------------------
    owner /tmp/dpkg-deb.@{TMP6}/{,**}			w,	# IMPROVE! Потом работать с xattr, чтобы ограничить только на процесс.
  }
}

# IMPROVE! Сейчас (модуль ядра 3.6, утилита 2.11.0) не поддерживаются
# множественные вложенные профили, выносим в отдельный профиль.
profile engrampa_tar_shell_root flags=(attach_disconnected) {
  #include <abstractions/base>
  #include <abstractions-root/fs/access-by-users-preset-rwkl>
  #include <abstractions-root/fs/access-by-pattern-systemusers>
  #include <abstractions/fs/deny-by-pattern-internet>
  #include <abstractions/fs/deny-private-strict>
  
  # PSEUDO ---------------------------------------------
  /dev/tty						rw,
  
  # EXECUTABLES ----------------------------------------
  @{shell}						mr,
  /bin/gzip@{t_root}					Cx,	# app-arch/gzip
  /bin/bzip2@{t_root}					Cx,	# app-arch/bzip2
  /usr/bin/lzip@{t_root}				Cx,	# app-arch/lzip
  /usr/bin/xz@{t_root}					Cx,	# app-arch/xz-utils
  
  # DENY INHERIT ---------------------------------------
  deny /apparmor/.null					rw,	# flags=(attach_disconnected)
  
  profile gzip /bin/gzip@{t_root} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/gzip						mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
  
  profile bzip2 /bin/bzip2@{t_root} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /bin/bzip2						mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
  
  profile lzip /usr/bin/lzip@{t_root} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/lzip					mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
  
  profile xz /usr/bin/xz@{t_root} flags=(attach_disconnected) {
    #include <abstractions/base>
    #include <abstractions-root/fs/access-by-users-preset-rwkl>
    #include <abstractions-root/fs/access-by-pattern-systemusers>
    #include <abstractions/fs/deny-by-pattern-internet>
    #include <abstractions/fs/deny-private-strict>
    
    # EXECUTABLES --------------------------------------
    /usr/bin/xz						mr,
    
    # DENY INHERIT -------------------------------------
    deny /apparmor/.null				rw,	# flags=(attach_disconnected)
  }
}

