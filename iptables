#include <tunables/global>
profile iptables.xtables-multi /sbin/xtables-multi flags=(complain) {
  #include <abstractions/base>
  #include <abstractions/nameservice>
  
  capability net_raw,
  capability net_admin,
  
  /sbin/xtables-multi r,
  
}
