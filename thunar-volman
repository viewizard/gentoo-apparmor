#include <tunables/global>
profile thunar-volman /usr/bin/thunar-volman flags=(complain) {
  #include <abstractions/base>
  #include <abstractions/freedesktop.org>
  #include <abstractions/fonts>
  #include <abstractions/X>
  #include <abstractions/gtk>
  
  @{PROC}/@{pid}/fd/ r,
  
  /usr/bin/thunar-volman r,
  /usr/bin/thunar-volman-settings Px,
  
}


profile thunar-volman-settings /usr/bin/thunar-volman-settings flags=(complain) {
  #include <abstractions/base>
  #include <abstractions/freedesktop.org>
  #include <abstractions/fonts>
  #include <abstractions/X>
  #include <abstractions/gtk>
  
  /usr/bin/thunar-volman-settings r,
  
}
