#include <tunables/global>
profile gnupg.gpg-agent /usr/bin/gpg-agent {
  #include <abstractions/base>
  #include <abstractions/user-tmp>
  
  /usr/bin/gpg-agent r,
  
  owner @{HOME}/.cache/gpg-agent-info rw,
  
}
