dbus send
	bus=session
	path="/org/gnome/GConf/Server"
	interface="org.gnome.GConf.Server"
	peer=(name="org.gnome.GConf",label="gconf.gconfd-2@{p_any}"),
dbus receive
	bus=session
	path="/org/gnome/GConf/Server"
	interface="org.gnome.GConf.Server"
	peer=(name=":[0-9]*.[0-9]*",label="gconf.gconfd-2@{p_any}"),
dbus send
	bus=session
	path="/org/gnome/GConf/Database/[0-9]*"
	interface="org.gnome.GConf.Database"
	peer=(name="org.gnome.GConf",label="gconf.gconfd-2@{p_any}"),
