#include <tunables/global>
profile at-spi-bus-launcher /usr/libexec/at-spi-bus-launcher {
  #include <abstractions/base>
  #include <abstractions/dbus>
  #include <abstractions/dconf>
  #include <abstractions/gtk>
  
  owner @{PROC}/@{pid}/fd/ r,
  
  /usr/libexec/at-spi-bus-launcher mr,
  /usr/bin/dbus-daemon Px,
  
  /etc/at-spi2/accessibility.conf r,
  
  owner @{HOME}/.session.log w,
  
}

profile at-spi2-registryd /usr/libexec/at-spi2-registryd {
  #include <abstractions/base>
  
  /usr/libexec/at-spi2-registryd mr,
  
  owner @{HOME}/.session.log w,
  
}
