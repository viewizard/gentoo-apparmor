#include <tunables/global>
profile gcc-config /usr/bin/gcc-config flags=(complain) {
  #include <abstractions/base>
  
  /dev/tty rw,
  
  /usr/bin/gcc-config r,
  /bin/bash mr,
  /usr/bin/python-wrapper Px,
  /usr/lib{,32,64}/python-exec/python-exec2 Cx -> portageq,
  
  /etc/env.d/gcc/* r,
  
  
  profile portageq flags=(complain) {
    #include <abstractions/base>
    
    /usr/lib{,32,64}/python-exec/python-exec2 m,
    /usr/lib{,32,64}/python-exec/python-exec2-c m,
    /bin/env r,
    
    /usr/lib{,32,64}/python-exec/@{PYTHON}/portageq Px,
    
    /etc/env.d/python/config r,
    
  }
  
}
