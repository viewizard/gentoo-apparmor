#include <tunables/global>
profile java-config @{PYTHON_EXEC}/java-config-2 flags=(complain) {
  #include <abstractions/base>
  #include <abstractions/python>
  
  /dev/pts/[0-9]* w,
  
  /etc/java-config-2/current-system-vm w,
  
  /usr/share/java-config-2/vm/{,**} r,
  
}
