  /usr/portage/**					r,
  /usr/local/portage/**					r,
  /usr/share/portage/**					r,
  
  # TEMP -----------------------------------------------
  audit owner /tmp/dispatch-conf.*/			rw,	# FIX ME! audit 26.11.2017
  
  profile diff /usr/bin/diff@{t_root} {
    #include <abstractions/base>
    
    # PSEUDO -------------------------------------------
    /dev/tty[0-9]*					rw,
    /dev/pts/[0-9]*					rw,
    
    # EXECUTABLES --------------------------------------
