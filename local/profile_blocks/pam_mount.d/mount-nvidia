# MOUNT ----------------------------------------------
mount fstype=tmpfs options=(rw, noexec) none -> /home/*.nv/,
mount fstype=tmpfs options=(rw, noexec) none -> /home/*/.nv/,
umount /home/*.nv/,
umount /home/*/.nv/,
