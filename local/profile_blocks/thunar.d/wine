# Запуск с установкой нужного WINEPREFIX.

# EXECUTABLES ------------------------------------------
/bin/env						Cx,

profile env /bin/env {
  #include <abstractions/base>
  
  # PSEUDO ---------------------------------------------
  /dev/tty[0-9]*					rw,
  /dev/pts/[0-9]*					rw,
  
  # EXECUTABLES ----------------------------------------
  /bin/env						mr,
  /usr/bin/wine						Px,	# app-emulation/wine
}
